{
  "name": "gulp-muxml",
  "version": "1.0.0",
  "description": "An XML parsing and formatting plugin for gulp",
  "license": "MIT",
  "repository": "t1st3/gulp-muxml",
  "author": {
    "name": "t1st3",
    "email": "contact@t1st3.com",
    "url": "t1st3.com"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo index.js test.js && mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov mocha",
    "precodecov": "npm run coverage",
    "codecov": "cat coverage/lcov.info | ./node_modules/.bin/codecov"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "xml",
    "parse",
    "format",
    "minify",
    "prettify",
    "strip",
    "filter",
    "stream",
    "transform",
    "through"
  ],
  "dependencies": {
    "concat-stream": "^2.0.0",
    "into-stream": "^4.0.0",
    "muxml": "^1.0.0",
    "through2": "^3.0.0"
  },
  "devDependencies": {
    "codecov": "^3.0.1",
    "mocha": "^5.1.1",
    "nyc": "^12.0.2",
    "vinyl": "^2.0.0",
    "xo": "0.23.0"
  },
  "clinton": {
    "rules": {
      "ava": false
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true
  }
}
