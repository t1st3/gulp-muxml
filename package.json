{
  "name": "gulp-muxml",
  "version": "0.1.2",
  "description": "An XML parsing and formatting plugin for gulp",
  "license": "MIT",
  "repository": "t1st3/gulp-muxml",
  "author": {
    "name": "t1st3",
    "email": "contact@tiste.org",
    "url": "tiste.org"
  },
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo index.js test.js && mocha",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov mocha",
    "precoveralls": "npm run coverage",
    "coveralls": "cat coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin",
    "xml",
    "parse",
    "format",
    "minify",
    "prettify",
    "strip",
    "filter",
    "stream",
    "transform",
    "through"
  ],
  "dependencies": {
    "concat-stream": "^1.5.2",
    "into-stream": "^3.0.0",
    "muxml": "^0.2.2",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.14",
    "mocha": "^3.0.2",
    "nyc": "^9.0.1",
    "vinyl": "^2.0.0",
    "xo": "*"
  },
  "clinton": {
    "rules": {
      "ava": false
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "esnext": true
  }
}
